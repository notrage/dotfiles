# This SSH config file can be passed to 'ssh -F'.
# This file is created by Lima, but not used by Lima itself currently.
# Modifications to this file will be lost on restarting the Lima instance.
Host lima-ubu-x86
  IdentityFile "/Users/dylan/.lima/_config/user"
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  NoHostAuthenticationForLocalhost yes
  GSSAPIAuthentication no
  PreferredAuthentications publickey
  Compression no
  BatchMode yes
  IdentitiesOnly yes
  Ciphers "^aes128-gcm@openssh.com,aes256-gcm@openssh.com"
  User dylan
  ControlMaster auto
  ControlPath "/Users/dylan/.lima/ubu_x86/ssh.sock"
  ControlPersist yes
  Hostname 127.0.0.1
  Port 55514
